#!/usr/bin/env bash

set -xeuo pipefail

sudo bash -c '
    set -xeuo pipefail
    rm -rf /opt/rancher-desktop
    rm -f /usr/local/bin/rancher-desktop
    rm -f /usr/local/share/applications/rancher-desktop.desktop
'

echo "****************************************"
echo "--Complete--"
echo "Rancher Desktop has been removed from the system!"
