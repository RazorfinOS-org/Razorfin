#!/usr/bin/env bash

set -xeuo pipefail

sudo bash -c '
    set -xeuo pipefail
    rm -rf /opt/VSCode-linux-x64
    rm /usr/local/bin/code
    rm /usr/local/share/applications/vscode.desktop
'

echo "****************************************"
echo "--Complete--"
echo "VSCode has been removed from the system!"
